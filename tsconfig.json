        "pod/dist"
      ],
      "pod/ngx": [
        "pod/dist/ngx"
      ],
      "fef-dynamic-layout": [
        "fef-dynamic-layout/dist"
      ],
      "fef-dynamic-layout/ng1": [
        "fef-dynamic-layout/dist/ng1"
      ],
      "ping-pong": [
        "ping-pong/dist"
      ],
      "ping-pong/ng1": [
        "ping-pong/dist/ng1"
      ]
    }
  }
}